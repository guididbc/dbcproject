{
	"name": "pl_system_to_bronze_csv",
	"properties": {
		"activities": [
			{
				"name": "Ler arquivos",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "ds_system_csv",
						"type": "DatasetReference",
						"parameters": {
							"container": "sistema/csvfiles",
							"filename": "*.csv"
						}
					},
					"fieldList": [
						"childItems"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			},
			{
				"name": "Remove arquivo",
				"type": "Filter",
				"dependsOn": [
					{
						"activity": "Ler arquivos",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Ler arquivos').output.childItems",
						"type": "Expression"
					},
					"condition": {
						"value": "@not(equals(item().name, 'out123.csv'))",
						"type": "Expression"
					}
				}
			}
		],
		"folder": {
			"name": "biamarostica - PL/pr√°tica1"
		},
		"annotations": []
	}
}